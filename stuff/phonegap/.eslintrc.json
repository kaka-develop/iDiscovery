{
    "env": {
        "node": true,
        "browser": true
    },
    "extends": [
        "eslint:recommended",
        "angular"
    ],
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}